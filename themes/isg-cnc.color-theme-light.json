{
    "$schema": "vscode://schemas/color-theme",
    "type": "light",
    "colors": {
        "activityBar.background": "#02351f",
        "editor.background": "#f5f5f5",
        "editor.foreground": "#333333",
        "titleBar.activeBackground": "#034b2b",
        "titleBar.activeForeground": "#edfef7"
    },
    "tokenColors": [
        {
            "scope": "keyword.command.gcode",
            "settings": {
                "foreground": "#5d009b",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.command.mcode",
            "settings": {
                "foreground": "#1900ff",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.command.hashtag",
            "settings": {
                "foreground": "#005e8a",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#0059ff",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": ""
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#000000",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "storage.type.programnumber",
            "settings": {
                "foreground": "#ff6600",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keywords.other.technology",
            "settings": {
                "foreground": "#486e00",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "storage.type.blocknumber",
            "settings": {
                "foreground": "#727272",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "keyword.axis",
            "settings": {
                "foreground": "#ff8800",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.tool",
            "settings": {
                "foreground": "#008871",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.call",
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "comment.line",
            "settings": {
                "foreground": "#3ba001",
                "fontStyle": ""
            }
        },
        {
            "scope": "comment.block",
            "settings": {
                "foreground": "#266402",
                "fontStyle": ""
            }
        },
        {
            "scope" : "variable.parameter",
            "settings": {
                "foreground": "#0a9ff5",
                "fontStyle": ""
            }
        },
        {
            "scope" : "string.quoted.double",
            "settings": {
                "foreground": "#96712d",
                "fontStyle": "bold"
            }
        }
    ]
}