{
    "$schema": "vscode://schemas/color-theme",
    "type": "dark",
    "colors": {
        "activityBar.background": "#02351f",
        "activityBarBadge.background": "#007acc",
        "editor.background": "#1e1e1e",
        "editor.foreground": "#d4d4d4",
        "editor.inactiveSelectionBackground": "#3a3d41",
        "editor.selectionHighlightBackground": "#add6ff26",
        "editorIndentGuide.activeBackground": "#707070",
        "editorIndentGuide.background": "#404040",
        "input.placeholderForeground": "#a6a6a6",
        "list.dropBackground": "#383b3d",
        "menu.background": "#252526",
        "menu.foreground": "#cccccc",
        "settings.numberInputBackground": "#292929",
        "settings.textInputBackground": "#292929",
        "sideBarTitle.foreground": "#bbbbbb",
        "statusBarItem.remoteBackground": "#16825d",
        "statusBarItem.remoteForeground": "#ffffff",
        "titleBar.activeBackground": "#034b2b",
        "titleBar.activeForeground": "#edfef7"
    },
    "tokenColors": [
        {
            "scope": "keyword.command.gcode",
            "settings": {
                "foreground": "#ce87fd",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.command.mcode",
            "settings": {
                "foreground": "#9665f3",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.command.hashtag",
            "settings": {
                "foreground": "#2eb9fa",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#0059ff",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": ""
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#a00078",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "storage.type.programnumber",
            "settings": {
                "foreground": "#486e00",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keywords.other.technology",
            "settings": {
                "foreground": "#ff6600",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "storage.type.blocknumber",
            "settings": {
                "foreground": "#727272",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "keyword.axis",
            "settings": {
                "foreground": "#ffbd2f",
                "fontStyle": ""
            }
        },
        {
            "scope": "keyword.tool",
            "settings": {
                "foreground": "#008871",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.control.call",
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "comment.line",
            "settings": {
                "foreground": "#3ba001",
                "fontStyle": ""
            }
        },
        {
            "scope": "comment.block",
            "settings": {
                "foreground": "#72dd34",
                "fontStyle": ""
            }
        },
        {
            "scope" : "variable.parameter",
            "settings": {
                "foreground": "#e523ff",
                "fontStyle": ""
            }
        },
        {
            "scope" : "string.quoted.double",
            "settings": {
                "foreground": "#96712d",
                "fontStyle": "bold"
            }
        }
    ]
}