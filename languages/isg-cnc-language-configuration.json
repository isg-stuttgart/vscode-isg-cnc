{
    "comments": {
        "lineComment": ";",
        "blockComment": [
            "#COMMENT BEGIN",
            "#COMMENT END"
        ]
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "'",
            "close": "'",
            "notIn": [
                "string",
                "comment"
            ]
        },
        {
            "open": "\"",
            "close": "\"",
            "notIn": [
                "string",
                "comment"
            ]
        },
        {
            "open": "`",
            "close": "`",
            "notIn": [
                "string",
                "comment"
            ]
        }
    ],
    "autoCloseBefore": ";:.,=}])>` \n\t",
    "surroundingPairs": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ],
        [
            "\"",
            "\""
        ],
        [
            "'",
            "'"
        ]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^(N\\d*)? *(#VAR|#COMMENT BEGIN|\\$DO|\\$REPEAT|\\$FOR|\\$IF|\\$WHILE|\\$CASE|\\$DEFAULT|\\$ELSE|\\$ELSEIF|\\$SWITCH)",
        "decreaseIndentPattern": "^(N\\d*)? *(#ENDVAR|#COMMENT END|\\$ENDDO|\\$UNTIL|\\$ENDFOR|\\$ENDIF|\\$ENDWHILE|\\$ENDSWITCH|\\$BREAK)",
        "indentNextLinePattern": "\\"
    },
    "folding": {
        "markers": {
            "start": "^(N\\d*)? *(#VAR|#COMMENT BEGIN|\\$DO|\\$REPEAT|\\$FOR|\\$IF|\\$WHILE|\\$SWITCH|\\$CASE|;#region)",
            "end": "^(N\\d*)? *(#ENDVAR|#COMMENT END|\\$ENDDO|\\$UNTIL|\\$ENDFOR|\\$ENDIF|\\$ENDWHILE|\\$ENDSWITCH|\\$BREAK|;#endregion)"
        }
    }
}